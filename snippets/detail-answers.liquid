              <ul class="branch-one clearfix">
                <li>
                  <div class="f-sub-child-one has-no-child">{{ product.metafields.custom.lineagechlid1 }}</div>
                </li>
                <li>
                  <div class="f-sub-child-two has-no-child">{{ product.metafields.custom.lineagechild2 }}</div>
                </li>
              </ul>
            </li>
          </ul>
        </div>
      </div>
      <button class="accordion">

<script>
  let shop = document.getElementById('shop-all');
  let childMenu = document.querySelector('#navigation-mobile .child-menu');
  let parentMenu = document.querySelector('#navigation-mobile .menu_list');
  let arrowLeft = document.querySelector('button.arrow-left');
  console.log({ childMenu, parentMenu });

  // Add an event listener for the 'click' event
  shop.addEventListener('click', function (event) {
    event.preventDefault(); // Prevent the default behavior of the anchor tag
    childMenu.classList.add('active');
    parentMenu.style.display = 'none';
    arrowLeft.style.display = 'block';
  });

  arrowLeft.addEventListener('click', function (event) {
    event.preventDefault();
    childMenu.classList.remove('active');
    setTimeout(() => {
      parentMenu.style.display = 'block';
      arrowLeft.style.display = 'none';
    }, 300);
  });

  const coll = document.getElementById('nav_shop_all');
  const content = document.querySelector('.shop-menu');
  const stickyHeader = document.querySelector('#sticky_header');

  const handleMouseEnter = () => {
    content.classList.remove('collapsed');
    content.classList.add('expanded');
    stickyHeader.classList.add('active-header');
  };

  const handleMouseLeave = () => {
    content.classList.remove('expanded');
    content.classList.add('collapsed');
    stickyHeader.classList.remove('active-header');
  };

  content.addEventListener('mouseenter', handleMouseEnter);
  content.addEventListener('mouseleave', handleMouseLeave);
  coll.addEventListener('mouseenter', handleMouseEnter);
  coll.addEventListener('mouseleave', handleMouseLeave);
</script>

  

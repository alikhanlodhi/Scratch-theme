/*# sourceMappingURL=/cdn/shop/t/42/assets/new-register-text-image.css.map?v=18112663111682347481681238713 */
.register-text-img-section-heading {
    margin-bottom: 53px;
    line-height: 100px;
    height: 105px;
}
.text-image-heading {
    font-family: 'RBNo3.1-Bold';
    font-weight: 450;
    font-size: 49px;
    line-height: 1.1;
    color: #FFFFFF;
    margin: 0;
}
.text-image-heading {
    margin-top: 20px;
}
.text-image-subheading {
    font-family: 'Montserrat', sans-serif;
    font-weight: 600;
    font-size: 16px;
    line-height: 1.8;    
    color: #FFFFFF;
    margin-bottom: 0 !important;
    margin-top: 20px;
    display: none;
}
.text-image-block {
    background: url("https://cdn.shopify.com/s/files/1/0831/9474/1040/files/rectangle-desktop.png?v=1696940920");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    height: 100%;
}
.swiper-horizontal {
    height: 539px;
}
.text-image-block-inner {
    padding: 49px 38px 42px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    height: 100%;
}
.text-image-block-img {
    display: flex;
    justify-content: center;
    align-items: center;
    max-height: 185px;
    height: 100%;
    max-width: 300px;
    /* width: 100%; */
}
.text-image-block-content {
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
}
.text-image-block-img img {
    height: 100%;
    width: 290px;
    max-width: 100%;
    object-fit: contain;
}
.resgister-text-image-section {
    margin-top: 20px;
    overflow: hidden;
}
/* swiper css */
.new_register_text_image {
    --swiper-scrollbar-border-radius: 10px;
    --swiper-scrollbar-top: auto;
    --swiper-scrollbar-bottom: -80px;
    --swiper-scrollbar-left: auto;
    --swiper-scrollbar-right: 4px;
    --swiper-scrollbar-sides-offset: 36%;
    --swiper-scrollbar-bg-color: #EAEAEA;
    --swiper-scrollbar-drag-bg-color: #B93AC6;
    --swiper-scrollbar-size: 5px;
}
.swiper, swiper-container {
    overflow: visible;
    cursor: grabbing;
}
.swiper_scrollbar {
    left: 50%;
    transform: translateX(-50%);
    background: #fff;
}

/* swiper css */

/* responsive */
@media (max-width: 1540px) {  
    .text-image-heading {
        font-size: 45px;
    }
}
@media (min-width: 1101px) {
    .text-image-block {
        transition: transform 0.4s ease;
    }
    .text-image-block-img {
        margin-top: 40px;
        margin-bottom: 40px;
        transition: 0.4s ease;
    }
    swiper-slide:hover .text-image-block{
        transform: translateY(-30px) !important;
        transition: transform 0.4s ease;
        filter: drop-shadow(-9px 8px 18px rgba(171, 171, 171, 0.2));
    } 
    .text-image-subheading {
        display: none;
        opacity: 0;
        visibility: hidden;
        height: 0;
        transition: 0.4s ease;
        -webkit-animation: slide-in-bottom 0.4s ease;
	        animation: slide-in-bottom 0.4s ease;
    }
    swiper-slide:hover .text-image-subheading {
        display: block;
        opacity: 1;
        visibility: visible;
        height: 100%;
        margin-top: 20px;
        -webkit-animation: slide-in-bottom 0.4s ease;
	        animation: slide-in-bottom 0.4s ease;
    }
    swiper-slide:hover .text-image-block-img {
        margin-top: 15px;
        margin-bottom: 15px;
        transition: 0.4s ease;
    }
    .text-image-heading {
        transition: 0.4s ease;
    }
    swiper-slide:hover .text-image-heading {
        transition: 0.4s ease;
    } 
    .text-image-block-inner {
        padding: 36px 38px 20px 38px;
        overflow: hidden;
        transition: 0.4s ease;
    } 
}
@media (min-width: 769px) and (max-width: 1100px) {  
    .text-image-block-inner {
        padding: 42px 35px;
    }  
}
@media (min-width: 769px) {  
    .resgister-text-image-section {
        height: 785px;
    }  
}

@media (max-width: 1100px) {
    .text-image-subheading {
        display: block;
    }
}
@media (max-width: 991px) {
    .resgister-text-image-section {
        margin-top: 60px;
    }
    .register-text-img-section-heading {
        margin-bottom: 50px;
    }
}

@media (max-width: 768px) {
    .register-text-img-section-heading {
        margin-bottom: 40px;
    }
    .text-image-heading, 
    .text-image-heading a {
        font-size: 36px;
    }
    .text-image-block-img {
        max-height: 142px;
        margin: 0;
    }
    .resgister-text-image-section {
        margin-top: 0;
    }
    .text-image-block {
        background: url('rectangle-mobile.png');
        background-repeat: no-repeat;
        background-size: 100% 100%;
        transition: 0.3s ease;
    }
    .text-image-block-inner {
        padding: 36px 37px 20px 37px;
    }
    .text-image-heading, .text-image-subheading {
        margin-top: 10px;
    }
    ul.list-a.footer_menu2{
        min-height: 212px;
    }
   .text-image-block-content {
        width: 100%;
    }
    .swiper-horizontal {
        height: 410px;
    }

}

@media screen and (min-width:430px) and (max-width: 768px) {
    .resgister-text-image-section {
        height: 730px;
    }
    .text-image-block {
        height: 510px;
    }
    .swiper-horizontal {
        height: 490px;
    }
}
@media (max-width: 601px) {
    .new_register_text_image {
        --swiper-scrollbar-sides-offset: 13%;
    }
    .resgister-text-image-section {
        height: 690px;
    }
    .text-image-block {
        margin-left: 0px;
        margin-right: 0px;
    }
    .register-text-img-section-heading {
        margin-bottom: 48px;
    }
    .text-image-block {
        height: 459px;
    }
    .resgister-text-image-section {
        height: 666px;
    }
}
@media screen and (max-width: 380px) {
    .text-image-block {
        height: 435px;
    }
    .text-image-block-inner {
        padding: 30px 37px 20px;
    }
}
@media screen and (max-width: 330px) {
    .text-image-block-inner {
        padding: 30px 30px 20px;
    }
    .swiper-horizontal {
        height: 470px;
    }
    .text-image-block {
        height: 500px;
    }
}
.slide-in-bottom {
	-webkit-animation: slide-in-bottom 0.4s ease;
	        animation: slide-in-bottom 0.4s ease;
}

@-webkit-keyframes slide-in-bottom {
  0% {
    -webkit-transform: translateY(100px);
            transform: translateY(100px);
    opacity: 0;
  }
  100% {
    -webkit-transform: translateY(0);
            transform: translateY(0);
    opacity: 1;
  }
}
@keyframes slide-in-bottom {
  0% {
    -webkit-transform: translateY(100px);
            transform: translateY(100px);
    opacity: 0;
  }
  100% {
    -webkit-transform: translateY(0);
            transform: translateY(0);
    opacity: 1;
  }
}
